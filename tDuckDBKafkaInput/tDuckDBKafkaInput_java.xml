<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="1.0"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="ESDB/Query.Farm"
		RELEASE_DATE="20251207"
		STARTABLE="true"
		LOG4J_ENABLED="true"
		HAS_CONDITIONAL_OUTPUTS="false"
		DATA_AUTO_PROPAGATE="false"
		SCHEMA_AUTO_PROPAGATE="false"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>ESDB/DuckDB</FAMILY>
		<FAMILY>Messaging/Kafka</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="0" MAX_OUTPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5" REQUIRED="true">
			<DEFAULT/>
		</PARAMETER>

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" NUM_ROW="10" FILTER="tDuckDBKafkaConnection" SHOW_IF="(USE_EXISTING_CONNECTION == 'true')"/>

		<PARAMETER NAME="BOOTSTRAP_SERVERS" FIELD="TEXT" NUM_ROW="15" REQUIRED="true" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>"localhost:9092"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TOPIC" FIELD="TEXT" NUM_ROW="20" REQUIRED="true">
			<DEFAULT>"my-topic"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SECURITY_PROTOCOL" FIELD="CLOSED_LIST" NUM_ROW="25" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<ITEMS DEFAULT="PLAINTEXT">
				<ITEM NAME="PLAINTEXT" VALUE="PLAINTEXT"/>
				<ITEM NAME="SSL" VALUE="SSL"/>
				<ITEM NAME="SASL_PLAINTEXT" VALUE="SASL_PLAINTEXT"/>
				<ITEM NAME="SASL_SSL" VALUE="SASL_SSL"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SASL_MECHANISM" FIELD="CLOSED_LIST" NUM_ROW="30" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SASL_PLAINTEXT' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<ITEMS DEFAULT="PLAIN">
				<ITEM NAME="PLAIN" VALUE="PLAIN"/>
				<ITEM NAME="SCRAM-SHA-256" VALUE="SCRAM-SHA-256"/>
				<ITEM NAME="SCRAM-SHA-512" VALUE="SCRAM-SHA-512"/>
				<ITEM NAME="GSSAPI" VALUE="GSSAPI"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SASL_USERNAME" FIELD="TEXT" NUM_ROW="35" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SASL_PLAINTEXT' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SASL_PASSWORD" FIELD="PASSWORD" NUM_ROW="35" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SASL_PLAINTEXT' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_ID" FIELD="TEXT" NUM_ROW="40">
			<DEFAULT>"talend-consumer-group"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="AUTO_OFFSET_RESET" FIELD="CLOSED_LIST" NUM_ROW="45">
			<ITEMS DEFAULT="earliest">
				<ITEM NAME="earliest" VALUE="earliest"/>
				<ITEM NAME="latest" VALUE="latest"/>
				<ITEM NAME="none" VALUE="none"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="MAX_MESSAGES" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"-1"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TIMEOUT_MS" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"30000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MESSAGE_FORMAT" FIELD="CLOSED_LIST" NUM_ROW="55">
			<ITEMS DEFAULT="JSON">
				<ITEM NAME="JSON" VALUE="JSON"/>
				<ITEM NAME="CSV" VALUE="CSV"/>
				<ITEM NAME="RAW" VALUE="RAW"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="INCLUDE_METADATA" FIELD="CHECK" NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="SPECIFIC_PARTITION" FIELD="TEXT" NUM_ROW="10">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SPECIFIC_OFFSET" FIELD="TEXT" NUM_ROW="10">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SSL_CA_LOCATION" FIELD="FILE" NUM_ROW="20" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SSL' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SSL_CERT_LOCATION" FIELD="FILE" NUM_ROW="25" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SSL' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SSL_KEY_LOCATION" FIELD="FILE" NUM_ROW="30" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SSL' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="EXTRA_KAFKA_PROPERTIES" FIELD="TABLE" NUM_ROW="40" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY_KEY" FIELD="TEXT"/>
				<ITEM NAME="PROPERTY_VALUE" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="DEBUG_MODE" FIELD="CHECK" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="DuckDB-Driver" MODULE="duckdb_jdbc-0.9.2.jar" MVN="mvn:org.duckdb/duckdb_jdbc/0.9.2" REQUIRED="true"/>
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
		<RETURN NAME="TOPIC" TYPE="id_String" AVAILABILITY="AFTER"/>
		<RETURN NAME="ERROR_MESSAGE" TYPE="id_String" AVAILABILITY="AFTER"/>
	</RETURNS>
</COMPONENT>
