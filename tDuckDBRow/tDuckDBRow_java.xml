<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="1.2"
    STATUS="PROD"
    COMPATIBILITY="ALL"
    AUTHOR="ESDB"
    RELEASE_DATE="20251130"
    STARTABLE="true"
    LOG4J_ENABLED="true"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>ESDB/DuckDB</FAMILY>
    <FAMILY>Big Data/Cloud</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" LINE_STYLE="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="MODE" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="5" GROUP="MODE_GROUP">
      <ITEMS DEFAULT="QUERY">
        <ITEM NAME="QUERY" VALUE="QUERY"/>
        <ITEM NAME="TRANSFORM" VALUE="TRANSFORM"/>
        <ITEM NAME="LOAD_FILE" VALUE="LOAD_FILE"/>
        <ITEM NAME="EXPORT" VALUE="EXPORT"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" REQUIRED="false" NUM_ROW="8" GROUP="CONNECTION_GROUP">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CONNECTION_COMPONENT" FIELD="COMPONENT_LIST" REQUIRED="true" NUM_ROW="8" GROUP="CONNECTION_GROUP" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" FILTER="tDuckDBConnection">
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER NAME="DATABASE_PATH" FIELD="FILE" REQUIRED="false" NUM_ROW="10" GROUP="CONNECTION_GROUP" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
      <DEFAULT>":memory:"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_CLOUD_STORAGE" FIELD="CHECK" REQUIRED="false" NUM_ROW="15" GROUP="CLOUD_GROUP">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CLOUD_PROVIDER" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="20" GROUP="CLOUD_GROUP" SHOW_IF="USE_CLOUD_STORAGE == 'true'">
      <ITEMS DEFAULT="S3">
        <ITEM NAME="S3" VALUE="S3"/>
        <ITEM NAME="AZURE" VALUE="AZURE"/>
        <ITEM NAME="GCS" VALUE="GCS"/>
        <ITEM NAME="R2" VALUE="R2"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="S3_ACCESS_KEY" FIELD="TEXT" REQUIRED="false" NUM_ROW="25" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'S3' OR CLOUD_PROVIDER == 'R2')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="S3_SECRET_KEY" FIELD="PASSWORD" REQUIRED="false" NUM_ROW="25" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'S3' OR CLOUD_PROVIDER == 'R2')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="S3_REGION" FIELD="TEXT" REQUIRED="false" NUM_ROW="30" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'S3')">
      <DEFAULT>"eu-west-1"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="S3_ENDPOINT" FIELD="TEXT" REQUIRED="false" NUM_ROW="30" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'S3' OR CLOUD_PROVIDER == 'R2')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="AZURE_CONNECTION_STRING" FIELD="PASSWORD" REQUIRED="false" NUM_ROW="25" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'AZURE')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="AZURE_ACCOUNT_NAME" FIELD="TEXT" REQUIRED="false" NUM_ROW="30" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'AZURE')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="GCS_KEY_FILE" FIELD="FILE" REQUIRED="false" NUM_ROW="25" GROUP="CLOUD_GROUP" SHOW_IF="(USE_CLOUD_STORAGE == 'true') AND (CLOUD_PROVIDER == 'GCS')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SQL_QUERY" FIELD="MEMO_SQL" REQUIRED="false" NUM_ROW="40" NB_LINES="8" GROUP="SQL_GROUP" SHOW_IF="MODE == 'QUERY' OR MODE == 'TRANSFORM'">
      <DEFAULT>"SELECT * FROM INPUT_BUFFER"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FILE_PATH" FIELD="TEXT" REQUIRED="false" NUM_ROW="45" GROUP="FILE_GROUP" SHOW_IF="MODE == 'LOAD_FILE'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FILE_FORMAT" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="45" GROUP="FILE_GROUP" SHOW_IF="MODE == 'LOAD_FILE' OR MODE == 'EXPORT'">
      <ITEMS DEFAULT="PARQUET">
        <ITEM NAME="PARQUET" VALUE="PARQUET"/>
        <ITEM NAME="CSV" VALUE="CSV"/>
        <ITEM NAME="JSON" VALUE="JSON"/>
        <ITEM NAME="NDJSON" VALUE="NDJSON"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="CSV_DELIMITER" FIELD="TEXT" REQUIRED="false" NUM_ROW="50" GROUP="FILE_GROUP" SHOW_IF="(MODE == 'LOAD_FILE' OR MODE == 'EXPORT') AND (FILE_FORMAT == 'CSV')">
      <DEFAULT>","</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CSV_HEADER" FIELD="CHECK" REQUIRED="false" NUM_ROW="50" GROUP="FILE_GROUP" SHOW_IF="(MODE == 'LOAD_FILE' OR MODE == 'EXPORT') AND (FILE_FORMAT == 'CSV')">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="EXPORT_PATH" FIELD="TEXT" REQUIRED="false" NUM_ROW="55" GROUP="EXPORT_GROUP" SHOW_IF="MODE == 'EXPORT'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="EXPORT_QUERY" FIELD="MEMO_SQL" REQUIRED="false" NUM_ROW="60" NB_LINES="5" GROUP="EXPORT_GROUP" SHOW_IF="MODE == 'EXPORT'">
      <DEFAULT>"SELECT * FROM INPUT_BUFFER"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="EXPORT_PARTITION_BY" FIELD="TEXT" REQUIRED="false" NUM_ROW="65" GROUP="EXPORT_GROUP" SHOW_IF="MODE == 'EXPORT'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="BATCH_SIZE" FIELD="TEXT" REQUIRED="false" NUM_ROW="70" GROUP="PROCESSING_GROUP">
      <DEFAULT>10000</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="80">
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="85" SHOW_IF="false">
      <DEFAULT>id_mysql</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" REQUIRED="false" NUM_ROW="90" GROUP="ERROR_GROUP">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>
    <PARAMETER NAME="MEMORY_LIMIT" FIELD="TEXT" REQUIRED="false" NUM_ROW="10" GROUP="DUCKDB_CONFIG">
      <DEFAULT>"4GB"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="THREADS" FIELD="TEXT" REQUIRED="false" NUM_ROW="10" GROUP="DUCKDB_CONFIG">
      <DEFAULT>4</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TEMP_DIRECTORY" FIELD="DIRECTORY" REQUIRED="false" NUM_ROW="15" GROUP="DUCKDB_CONFIG">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="EXTENSIONS" FIELD="TEXT" REQUIRED="false" NUM_ROW="20" GROUP="DUCKDB_CONFIG">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="INIT_SQL" FIELD="MEMO_SQL" REQUIRED="false" NUM_ROW="25" NB_LINES="4" GROUP="DUCKDB_CONFIG">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PRE_SQL" FIELD="MEMO_SQL" REQUIRED="false" NUM_ROW="30" NB_LINES="3" GROUP="HOOKS">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="POST_SQL" FIELD="MEMO_SQL" REQUIRED="false" NUM_ROW="35" NB_LINES="3" GROUP="HOOKS">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CLEANUP_SQL" FIELD="MEMO_SQL" REQUIRED="false" NUM_ROW="40" NB_LINES="3" GROUP="HOOKS">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_HTTPFS" FIELD="CHECK" REQUIRED="false" NUM_ROW="45" GROUP="EXTENSIONS_AUTO">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_SPATIAL" FIELD="CHECK" REQUIRED="false" NUM_ROW="45" GROUP="EXTENSIONS_AUTO">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_FTS" FIELD="CHECK" REQUIRED="false" NUM_ROW="45" GROUP="EXTENSIONS_AUTO">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_ICE" FIELD="CHECK" REQUIRED="false" NUM_ROW="50" GROUP="EXTENSIONS_AUTO">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_DELTA" FIELD="CHECK" REQUIRED="false" NUM_ROW="50" GROUP="EXTENSIONS_AUTO">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="DEBUG_MODE" FIELD="CHECK" REQUIRED="false" NUM_ROW="60" GROUP="DEBUG">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LOG_SQL" FIELD="CHECK" REQUIRED="false" NUM_ROW="60" GROUP="DEBUG">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PRESERVE_DB" FIELD="CHECK" REQUIRED="false" NUM_ROW="65" GROUP="DEBUG">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="DuckDB-Driver" MODULE="duckdb_jdbc-1.0.0.jar" MVN="mvn:org.duckdb/duckdb_jdbc/1.0.0" REQUIRED="true"/>
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE_INPUT" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_OUTPUT" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_BATCHES" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="PROCESSING_TIME_MS" TYPE="id_Long" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_FILES_LOADED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_FILES_EXPORTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_ERRORS" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
