<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="1.0"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="ESDB/Query.Farm"
		RELEASE_DATE="20251207"
		STARTABLE="false"
		LOG4J_ENABLED="true"
		HAS_CONDITIONAL_OUTPUTS="false"
		DATA_AUTO_PROPAGATE="true"
		SCHEMA_AUTO_PROPAGATE="true"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>ESDB/DuckDB</FAMILY>
		<FAMILY>Messaging/Kafka</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="0" MAX_OUTPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5" REQUIRED="true">
			<DEFAULT/>
		</PARAMETER>

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" NUM_ROW="10" FILTER="tDuckDBKafkaConnection" SHOW_IF="(USE_EXISTING_CONNECTION == 'true')"/>

		<PARAMETER NAME="BOOTSTRAP_SERVERS" FIELD="TEXT" NUM_ROW="15" REQUIRED="true" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>"localhost:9092"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TOPIC" FIELD="TEXT" NUM_ROW="20" REQUIRED="true">
			<DEFAULT>"my-topic"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SECURITY_PROTOCOL" FIELD="CLOSED_LIST" NUM_ROW="25" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<ITEMS DEFAULT="PLAINTEXT">
				<ITEM NAME="PLAINTEXT" VALUE="PLAINTEXT"/>
				<ITEM NAME="SSL" VALUE="SSL"/>
				<ITEM NAME="SASL_PLAINTEXT" VALUE="SASL_PLAINTEXT"/>
				<ITEM NAME="SASL_SSL" VALUE="SASL_SSL"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SASL_MECHANISM" FIELD="CLOSED_LIST" NUM_ROW="30" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SASL_PLAINTEXT' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<ITEMS DEFAULT="PLAIN">
				<ITEM NAME="PLAIN" VALUE="PLAIN"/>
				<ITEM NAME="SCRAM-SHA-256" VALUE="SCRAM-SHA-256"/>
				<ITEM NAME="SCRAM-SHA-512" VALUE="SCRAM-SHA-512"/>
				<ITEM NAME="GSSAPI" VALUE="GSSAPI"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SASL_USERNAME" FIELD="TEXT" NUM_ROW="35" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SASL_PLAINTEXT' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SASL_PASSWORD" FIELD="PASSWORD" NUM_ROW="35" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SASL_PLAINTEXT' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MESSAGE_FORMAT" FIELD="CLOSED_LIST" NUM_ROW="40">
			<ITEMS DEFAULT="JSON">
				<ITEM NAME="JSON" VALUE="JSON"/>
				<ITEM NAME="CSV" VALUE="CSV"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="KEY_COLUMN" FIELD="PREV_COLUMN_LIST" NUM_ROW="45">
			<DEFAULT/>
		</PARAMETER>

		<PARAMETER NAME="BATCH_SIZE" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"1000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LINGER_MS" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"100"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ACKS" FIELD="CLOSED_LIST" NUM_ROW="55">
			<ITEMS DEFAULT="all">
				<ITEM NAME="0" VALUE="0"/>
				<ITEM NAME="1" VALUE="1"/>
				<ITEM NAME="all" VALUE="all"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="COMPRESSION_TYPE" FIELD="CLOSED_LIST" NUM_ROW="60">
			<ITEMS DEFAULT="none">
				<ITEM NAME="none" VALUE="none"/>
				<ITEM NAME="gzip" VALUE="gzip"/>
				<ITEM NAME="snappy" VALUE="snappy"/>
				<ITEM NAME="lz4" VALUE="lz4"/>
				<ITEM NAME="zstd" VALUE="zstd"/>
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="SSL_CA_LOCATION" FIELD="FILE" NUM_ROW="10" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SSL' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SSL_CERT_LOCATION" FIELD="FILE" NUM_ROW="15" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SSL' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SSL_KEY_LOCATION" FIELD="FILE" NUM_ROW="20" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (SECURITY_PROTOCOL == 'SSL' OR SECURITY_PROTOCOL == 'SASL_SSL')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="EXTRA_KAFKA_PROPERTIES" FIELD="TABLE" NUM_ROW="30" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY_KEY" FIELD="TEXT"/>
				<ITEM NAME="PROPERTY_VALUE" FIELD="TEXT"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="RETRIES" FIELD="TEXT" NUM_ROW="40">
			<DEFAULT>"3"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="RETRY_BACKOFF_MS" FIELD="TEXT" NUM_ROW="40">
			<DEFAULT>"1000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BUFFER_MEMORY" FIELD="TEXT" NUM_ROW="45">
			<DEFAULT>"33554432"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAX_BLOCK_MS" FIELD="TEXT" NUM_ROW="45">
			<DEFAULT>"60000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEBUG_MODE" FIELD="CHECK" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="DuckDB-Driver" MODULE="duckdb_jdbc-0.9.2.jar" MVN="mvn:org.duckdb/duckdb_jdbc/0.9.2" REQUIRED="true"/>
			<IMPORT NAME="Kafka-Clients" MODULE="kafka-clients-3.6.1.jar" MVN="mvn:org.apache.kafka/kafka-clients/3.6.1" REQUIRED="true"/>
			<IMPORT NAME="JSON" MODULE="json-20231013.jar" MVN="mvn:org.json/json/20231013" REQUIRED="true"/>
			<IMPORT NAME="SLF4J-API" MODULE="slf4j-api-1.7.36.jar" MVN="mvn:org.slf4j/slf4j-api/1.7.36" REQUIRED="true"/>
			<IMPORT NAME="SLF4J-Simple" MODULE="slf4j-simple-1.7.36.jar" MVN="mvn:org.slf4j/slf4j-simple/1.7.36" REQUIRED="true"/>
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
		<RETURN NAME="TOPIC" TYPE="id_String" AVAILABILITY="AFTER"/>
		<RETURN NAME="ERROR_MESSAGE" TYPE="id_String" AVAILABILITY="AFTER"/>
	</RETURNS>
</COMPONENT>
